        </activity>
        <activity
            android:name=".activity.ModificarRevGral"
            android:label="@string/title_activity_modificarrevgral"
            android:parentActivityName=".activity.ListaLogs" >
            <!-- Parent activity meta-data to support API level 7+ -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.ListaLogs" />
        </activity>
        <activity
            android:name=".activity.AddItv"
            android:label="@string/title_activity_additv"
            android:parentActivityName=".activity.MyActivity" >
            <!-- Parent activity meta-data to support API level 7+ -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.MyActivity" />
        </activity>
        <activity android:name="com.facebook.LoginActivity" />
        <service android:name=".negocio.Notificaciones" />
    </application>

</manifest>
